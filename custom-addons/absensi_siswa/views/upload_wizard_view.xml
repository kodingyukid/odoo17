<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_upload_wizard_form" model="ir.ui.view">
        <field name="name">upload.wizard.form</field>
        <field name="model">upload.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="absensi_line_id" invisible="1"/>
                    <field name="attachment_ids" widget="many2many_binary" string="Pilih File..."/>
                </group>
                <footer>
                    <button name="action_confirm_upload" string="Upload" type="object" class="btn-primary"/>
                    <button string="Batal" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
