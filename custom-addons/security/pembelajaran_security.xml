<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="module_category_pembelajaran" model="ir.module.category">
            <field name="name">Pembelajaran</field>
            <field name="description">Hak akses untuk Modul Pembelajaran</field>
            <field name="sequence">20</field>
        </record>

        <record id="group_pembelajaran_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_pembelajaran"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="comment">Grup untuk manager yang bisa mengelola semua data pembelajaran.</field>
        </record>

    </data>
</odoo>
