async notify(message,channel){const modelsHandleByPush=["mail.thread","discuss.channel"];if(modelsHandleByPush.includes(message.model)&&(await this.hasServiceWorkInstalledAndPushSubscriptionActive())){return;}
const author=message.author;let notificationTitle;let icon="/mail/static/src/img/odoobot_transparent.png";if(!author){notificationTitle=_t("New message");}else{if(author.type==="partner"){icon=`/web/image/res.partner/${author.id}/avatar_128`;}
