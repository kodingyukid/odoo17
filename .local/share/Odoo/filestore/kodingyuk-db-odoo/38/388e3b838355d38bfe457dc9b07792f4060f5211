                <span t-elif="member.rtcSession?.isDeaf" class="p-1 fa fa-deaf"/>
                <span t-if="member.rtcSession?.raisingHand" class="p-1 fa fa-hand-paper-o"/>
            </span>

        </div>
    </t>

