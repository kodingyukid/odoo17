odoo.define('@mail/discuss/gif_picker/common/picker_patch',['@web/core/utils/patch','@mail/core/common/picker','@web/core/utils/misc'],function(require){'use strict';let __exports={};const{patch}=require("@web/core/utils/patch");const{Picker}=require("@mail/core/common/picker");const{isEventHandled}=require("@web/core/utils/misc");patch(Picker.prototype,{isEventHandledByPicker(ev){return super.isEventHandledByPicker(ev)||isEventHandled(ev,"Composer.onClickAddGif");},async toggle(el,ev){await super.toggle(el,ev);if(isEventHandled(ev,"Composer.onClickAddGif")){if(this.popover.isOpen){if(this.props.state.picker===this.props.PICKERS.GIF){this.close();return;}
