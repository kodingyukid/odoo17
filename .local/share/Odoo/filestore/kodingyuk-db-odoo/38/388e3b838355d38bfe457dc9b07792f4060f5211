odoo.define('@mail/core/common/persona_model',['@mail/core/common/record','@web/core/utils/timing'],function(require){'use strict';let __exports={};const{AND,Record}=require("@mail/core/common/record");const{debounce}=require("@web/core/utils/timing");const Persona=__exports.Persona=class Persona extends Record{static id=AND("type","id");static records={};static get(data){return super.get(data);}
